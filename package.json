{
  "name": "sfos-community-website",
  "version": "0.0.0",
  "description": "A community website created for fun.",
  "keywords": [
    "community-website",
    "daisy-ui",
    "tailwindcss"
  ],
  "homepage": "https://github.com/SFOS-Community/SFOS-Community.github.io#readme",
  "bugs": {
    "url": "https://github.com/SFOS-Community/SFOS-Community.github.io/issues",
    "email": "jordirwn@gmail.com"
  },
  "author": "Jordi Irawan <jordirwn@gmail.com> (https://wan.is-a.dev/)",
  "license": "MIT",
  "main": "./index.html",
  "repository": {
    "type": "git",
    "url": "https://github.com/SFOS-Community/SFOS-Community.github.io.git"
  },
  "scripts": {
    "dev": "npm run tailwind:watch",
    "prod": "npm run tailwind:minify",
    "tailwind:watch": "tailwindcss -i ./src/style.css -o ./public/css/style.css --watch",
    "tailwind:minify": "tailwindcss -i ./src/style.css -o ./public/css/style.css --minify",
    "format": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "devDependencies": {
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.11"
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "private": true,
  "dependencies": {
    "@tailwindcss/typography": "^0.5.2",
    "daisyui": "^2.15.3",
    "tailwindcss": "^3.1.2"
  }
}
